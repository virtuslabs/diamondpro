<!-- <h2>View Client</h2> -->
<div class="row">
    <div class="col-md-4">
      <div class="panel panel-default library-item">
          <div class="panel-heading">
              <h3 class="panel-title">{{model.last_name}}, {{model.first_name}}</h3>
          </div>
          <div class="panel-body">
            <p>First Name: {{model.first_name}}</p>
            <p>Last Name: {{model.last_name}}</p>
            <p>Address: {{model.address}}</p>
            <p>Phone: {{model.phone_number}}</p>
            <p>Email: {{model.email}}</p>
          </div>
          <div class="panel-footer text-right">
              {{#link-to 'admin.clients.client.edit' model.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
              <!-- <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' library}}>Delete</button> -->
          </div>
      </div>
    </div>
</div>
