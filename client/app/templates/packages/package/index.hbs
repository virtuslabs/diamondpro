<div class="row">
    <div class="col-md-4">

      <div class="panel panel-default">

          <div class="panel-heading">
              <h3 class="panel-title">{{model.title}}</h3>
          </div>
          <div class="panel-body">
            <p>Package Descripton: {{model.description}}</p>
            <p>Package Type: {{model.package_type}}</p>
            <p>Package Price: {{model.price}}</p>
            </div>
          <div class="panel-footer text-right">
              {{#link-to 'admin.packages.package.edit' model.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
              <!-- <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' library}}>Delete</button> -->
          </div>
      </div>
    </div>
</div>
