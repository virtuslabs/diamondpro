<div class="col-md-4">
  <div class="panel panel-default">
      <div class="panel-heading">
          <h3 class="panel-title">{{package.title}}</h3>
      </div>
      <div class="panel-body">
        <p>Package Descripton: {{package.description}}</p>
        <p>Package Type: {{package.package_type}}</p>
        <p>Package Price: {{package.price}}</p>
        </div>
      <div class="panel-footer text-right">
          {{#link-to 'admin.packages.package.edit' model.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
          <!-- <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' library}}>Delete</button> -->
      </div>
  </div>
</div>
{{yield}}
